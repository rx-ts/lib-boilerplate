{
  "name": "lib-boilerplate",
  "version": "0.0.2",
  "type": "module",
  "description": "A simple library boilerplate.",
  "repository": "git+https://github.com/un-ts/lib-boilerplate.git",
  "author": "JounQin (https://www.1stG.me) <admin@1stg.me>",
  "funding": "https://opencollective.com/unts",
  "license": "MIT",
  "packageManager": "pnpm@8.3.1",
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": "./src/index.ts",
  "files": [
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r -f cjs",
    "build:tsc": "tsc -p src",
    "dev": "vitest",
    "docs:build": "vite build docs",
    "docs:dev": "vite docs",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --cache -f friendly --max-warnings 10",
    "lint:style": "stylelint . --cache",
    "lint:tsc": "tsc --noEmit",
    "postversion": "pnpm i --no-frozen-lockfile",
    "prepare": "simple-git-hooks",
    "prerelease": "pnpm build",
    "release": "changeset publish",
    "serve": "vite preview docs",
    "test": "vitest run --coverage",
    "typecov": "type-coverage",
    "vercel-build": "pnpm docs:build",
    "version": "changeset version"
  },
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@1stg/app-config": "^8.1.0",
    "@1stg/lib-config": "^11.1.0",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@mdx-js/rollup": "^2.3.0",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/mdx": "^2.0.5",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/web": "^0.0.111",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "@vitest/coverage-istanbul": "^0.30.1",
    "github-markdown-css": "^5.2.0",
    "lib-boilerplate": "link:.",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "size-limit": "^8.2.6",
    "type-coverage": "^2.25.0",
    "typescript": "~5.0.4",
    "unplugin-auto-import": "^0.15.3",
    "vite": "^4.3.4",
    "vitest": "^0.30.1"
  },
  "publishConfig": {
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "exports": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "types": "./lib/index.d.ts"
  },
  "pnpm": {
    "patchedDependencies": {
      "@vitejs/plugin-react-swc@3.3.0": "patches/@vitejs__plugin-react-swc@3.3.0.patch"
    }
  },
  "size-limit": [
    {
      "path": "lib/index.js"
    }
  ],
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
